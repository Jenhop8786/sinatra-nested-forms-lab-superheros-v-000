<form action="/teams" method="POST">

<h1>Create a Team and Heroes!</h1>
<label for="team[name]">Team Name:</label>
<input type="text" name="team[name]">

<label for="team[motto]">Team Motto:</label>
<input type="text" name="team[motto]">

<h2>Hero 1</h2>
<label for="hero1_name">Hero's Name:</label>
<input id="hero1_name" type="text" name="team"[members][][name]">

<label for="hero1_power">Hero's Power:</label>
<input id="hero1_power" type="text" name="team"[members][][name]">

<label for="hero[biography]">Hero's Biography:</label>
<input type="text" name="hero[biography]">
